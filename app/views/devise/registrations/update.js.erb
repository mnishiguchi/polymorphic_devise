<% case @update_type %>
<% when :password %>
  console.log("update_type: password")
  $('#edit_password').html(
    '<%= j(render "edit_password") %>'
  )
<% when :email %>
  console.log("update_type: email")
  $('#edit_email').html(
    '<%= j(render "edit_email") %>'
  )
<% else %>
  console.log("update_type: else")
<% end %>

$('#flash_message').html(
  '<%= j(render "layouts/flash_message") %>'
)
